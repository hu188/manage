    <view class="title">累计分红金额</view>
    <view>￥{{price.price(20)}}</view>
  </view>
  <view class="income-list">
    <view class="title">可提现金额</view>
    <view>￥{{price.price(20)}}</view>
  </view>
</view>
<view class="income">
  <view class="income-list">
    <view class="title">待结算金额</view>
    <view>￥{{price.price(20)}}</view>
  </view>
  <view class="income-list">
    <view class="title">收益占比</view>
