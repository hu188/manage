  </view>
    <view class="income-list">
    <view class="title">待结算分红</view>
    <view>￥{{price.price(20)}}</view>
  </view>
</view>
<button class="btn" bindtap='exitLogin'>退出登录</button>
<i-tab current='income'></i-tab>